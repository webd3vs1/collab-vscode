{
    "name": "collab",
    "displayName": "collab",
    "description": "remotely work on a project at anytime",
    "version": "0.0.4",
    "repository": {
        "url": "https://github.com/MS-AZ/collab"
    },
    "publisher": "ms-az",
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:collab.load",
        "onCommand:collab.decorate",
        "onFileSystem:collab"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "collab.load",
                "title": "Load collaboration"
            },{
                "command": "collab.decorate",
                "title": "Collab: Decorate"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.65.0",
        "@types/ws": "^8.5.3",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "@vscode/test-electron": "^2.1.2",
        "eslint": "^8.9.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.1",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "axios": "^0.26.1",
        "cors": "^2.8.5",
        "express": "^4.17.3",
        "ws": "^8.5.0"
    }
}
